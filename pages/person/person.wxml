<view class="container">
    <!-- 个人信息 -->
    <view class="info">
        <view style="margin-left:6%">
            <van-image width="3rem" height="3rem" round />
        </view>
        <view style="margin-left:3%">一般</view>
    </view>
    <!-- 我的订单 -->
    <view class="myOrder mr">
        <van-row custom-class="orderTitle">我的订单</van-row>
        <van-row custom-class="row">
            <van-col span="6" custom-class="order" catch:tap="listTap">
                <view>
                    <van-image width="2.5rem" height="2.5rem" />
                </view>
                <view>待付款</view>
            </van-col>
            <van-col span="6" custom-class="order">
                <view>
                    <van-image width="2.5rem" height="2.5rem" />
                </view>
                <view>待配送</view>
            </van-col>
            <van-col span="6" custom-class="order">
                <view>
                    <van-image width="2.5rem" height="2.5rem" />
                </view>
                <view>待提货</view>
            </van-col>
            <van-col span="6" custom-class="order">
                <view>
                    <van-image width="2.5rem" height="2.5rem" />
                </view>
                <view>已完成</view>
            </van-col>
        </van-row>
    </view>
    <!-- cell -->
    <van-cell-group>
        <button open-type="contact">
            <van-cell title="团长信息" icon="location-o" ></van-cell>
        </button>
        <van-cell title="团长申请"></van-cell>
        <van-cell title="意见反馈"></van-cell>
        <van-cell title="联系客服"></van-cell>
    </van-cell-group>
</view>